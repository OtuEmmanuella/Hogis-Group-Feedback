{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "routes": [
      { "handle": "filesystem" },
      { "src": "/assets/(.*)", "dest": "/assets/$1" },
      { "src": "/images/(.*)", "dest": "/images/$1" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }